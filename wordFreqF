function [totalScore] = wordFreqF(extractedArticle)
% wordFreq.m
% Generates the total frequency of each word in an article. 
% Careful, if a word appears more than once, it shows up in
% the display more than once (eg. if a word appears twice, it shows
% up in the display twice). So, you should only look at words
% with more than "1" frequency the first time. 

words = strsplit(lower(extractedArticle));
reps = length(words);
count = zeros(1,reps);
for j = 1:reps
    if strcmp((words{j}(end)),'.') || strcmp((words{j}(end)),',') || strcmp((words{j}(end)),';') || strcmp((words{j}(end)),'?') || strcmp((words{j}(end)),'!') || strcmp((words{j}(end)),':') || strcmp((words{j}(end)),')') || strcmp((words{j}(end)),'%') || strcmp((words{j}(end)),'"')
        words{j} = words{j}(1:end-1);
    end
    if strcmp((words{j}(1)),'(') || strcmp((words{j}(1)),'"')
        words{j} = words{j}(2:end);
    end
end

key = containers.Map(words,count);


for i = 1:reps
    key(words{i}) = key(words{i}) + 1;
end

for m = 1:reps
  if key(words{m}) >= 50
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 49
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 48
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 47
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 46
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 45
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 44
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 43
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 42
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 41
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 40
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 39
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 38
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 37
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 36
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 35
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 34
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 33
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 32
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 31
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 30
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 29
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 28
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 27
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 26
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 25
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 24
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 23
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 22
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 21
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 20
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 19
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 18
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 17
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 16
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 15
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 14
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 13
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 12
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 11
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 10
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 9
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 8
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 7
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 6
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 5
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 4
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 3
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 2
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end

for m = 1:reps
  if key(words{m}) == 1
      disp([words{m}, ': ',num2str(key(words{m}))]);
  end
end
